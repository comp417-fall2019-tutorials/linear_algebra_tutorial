<?xml version='1.0'?>

<robot name="robot_model" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find linear_algebra_tutorial)/urdf/robot_model.gazebo" />
  <link name="robot_footprint"></link>

  <gazebo reference="chassis">
    <material>Gazebo/Orange</material>
  </gazebo>

  <gazebo reference="front_caster">
    <selfCollide>false</selfCollide>
    <material>Gazebo/Black</material>
    <mu1>0</mu1>
    <mu2>0</mu2>
    <minDepth>0.01</minDepth>
    <maxVel>1000.0</maxVel>
  </gazebo>

  <gazebo reference="back_caster">
    <selfCollide>false</selfCollide>
    <material>Gazebo/Black</material>
    <mu1>0</mu1>
    <mu2>0</mu2>
    <minDepth>0.01</minDepth>
    <maxVel>1000.0</maxVel>
  </gazebo>

  <gazebo reference="right_pad">
    <material>Gazebo/Orange</material>
  </gazebo>
  <gazebo reference="left_pad">
    <material>Gazebo/Orange</material>
  </gazebo>
  <gazebo reference="left_wheel">
    <selfCollid>false</selfCollid>
    <material>Gazebo/Black</material>
    <mu1>10</mu1>
    <mu2>10</mu2>
    <minDepth>0.01</minDepth>
    <maxVel>1000.0</maxVel>
  </gazebo>
  <gazebo reference="right_wheel">
    <selfCollid>false</selfCollid>
    <material>Gazebo/Black</material>
    <mu1>10</mu1>
    <mu2>10</mu2>
    <minDepth>0.01</minDepth>
    <maxVel>1000.0</maxVel>
  </gazebo>
  <gazebo reference="camera">
    <material>Gazebo/Black</material>
  </gazebo>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='chassis_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".4 .2 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 -0.4" rpy="1.57 0 0"/>
      <geometry>
        <mesh filename="package://linear_algebra_tutorial/meshes/duck/duck.dae"/>
      </geometry>
    </visual>


  </link>

  <link name="front_caster">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="2.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
      />
    </inertial>
    <collision name='front_caster_collision'>
      <origin xyz="0. 0. 0." rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </collision>

    <visual name='front_caster_visual'>
      <origin xyz="0. 0. 0." rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint type="fixed" name="front_caster_joint">
    <origin xyz="0.15 0 -0.05" rpy="0 0 0"/>
    <child link="front_caster"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <link name="back_caster">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="2.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
      />
    </inertial>
    <collision name='back_caster_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </collision>

    <visual name='back_caster_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint type="fixed" name="back_caster_joint">
    <origin xyz="-0.15 0 -0.05" rpy="0 0 0"/>
    <child link="back_caster"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <link name="left_wheel">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
      />
    </inertial>

    <visual name='left_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>

    <collision name='left_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>
  </link>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.14 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100000" velocity="10000"/>
    <dynamics damping="1.0" friction="0.0"/>
  </joint>


  <link name="right_wheel">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
              ixx="0.1" ixy="0" ixz="0"
              iyy="0.1" iyz="0"
              izz="0.1"
      />
    </inertial>

    <visual name='right_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </visual>

    <collision name='right_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>
  </link>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 -0.14 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100000" velocity="10000"/>
    <dynamics damping="1.0" friction="0.0"/>
  </joint>


  <!-- CAMERA LINK -->
  <link name="camera">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <inertia
              ixx="1e-6" ixy="0" ixz="0"
              iyy="1e-6" iyz="0"
              izz="1e-6"
      />
    </inertial>

    <visual name='camera_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </visual>

    <collision name='camera_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </collision>
  </link>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.2 0.0 -0.3" rpy="0 1.570796 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>


  <link name="right_pad">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia
              ixx="1e-6" ixy="0" ixz="0"
              iyy="1e-6" iyz="0"
              izz="1e-6"
      />
    </inertial>

    <visual name='right_pad_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.06 0.06 0.1"/>
      </geometry>
    </visual>

    <collision name='right_pad_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.06 0.06 0.1"/>
      </geometry>
    </collision>
  </link>

  <joint type="fixed" name="right_pad_joint">
    <origin xyz="-0.17 -0.13 0" rpy="0 0 0"/>
    <child link="right_pad"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <link name="left_pad">
    <pose>0 0 0 0 0 0</pose>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia
              ixx="1e-6" ixy="0" ixz="0"
              iyy="1e-6" iyz="0"
              izz="1e-6"
      />
    </inertial>

    <visual name='left_pad_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.06 0.06 0.1"/>
      </geometry>
    </visual>

    <collision name='left_pad_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.06 0.06 0.1"/>
      </geometry>
    </collision>
  </link>

  <joint type="fixed" name="left_pad_joint">
    <origin xyz="-0.17 0.13 0" rpy="0 0 0"/>
    <child link="left_pad"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

</robot>

